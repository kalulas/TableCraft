{
    "ConfigHomePath": "",
    "JsonHomePath": "",
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { 
                "Name": "Console" 
            },
            { 
                "Name": "File", 
                "Args": {
                    "path": "Logs/log-.txt",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 10
                }
            }
        ]
    }
}